{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
     "no-shadow": "off",
     "react/react-in-jsx-scope": "off",
     "react/function-component-definition": "off",
     "import/prefer-default-export": "off"
    }
}
